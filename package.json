    "react-dom": "15.3.2",
    "react-flexbox-grid": "^1.1.3",
    "react-google-button": "^0.1.0",
    "react-grid-system": "^2.7.1",
    "react-particles-js": "^1.5.0",
    "react-redux": "^4.4.5",
    "react-redux-firebase": "*",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "1.0.0",
